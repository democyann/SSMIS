<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="task" extends="json"  namespace="/autoFom">
		<action name="*" class="taskAction" method="{1}">
			<result name="index">/pages/task/index.jsp</result>
			<result name="subtask">/pages/task/subindex.jsp</result>
			<result name="taskinfo">/pages/task/taskinfo.jsp</result>
			<result name="json" type="json"></result>			
		</action>
	</package>
</struts>
